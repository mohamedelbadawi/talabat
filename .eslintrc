{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard-with-typescript",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi":[
            "error",
            "always"
        ],
        "indent":[
            "error",2
        ],
        "quotes":[
            "error",
            "double"
        ]
    }
}
